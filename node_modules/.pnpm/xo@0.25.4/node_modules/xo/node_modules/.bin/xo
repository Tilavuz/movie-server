#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tilav/codes/full project/movie/server/node_modules/.pnpm/xo@0.25.4/node_modules/xo/node_modules:/home/tilav/codes/full project/movie/server/node_modules/.pnpm/xo@0.25.4/node_modules:/home/tilav/codes/full project/movie/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tilav/codes/full project/movie/server/node_modules/.pnpm/xo@0.25.4/node_modules/xo/node_modules:/home/tilav/codes/full project/movie/server/node_modules/.pnpm/xo@0.25.4/node_modules:/home/tilav/codes/full project/movie/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
